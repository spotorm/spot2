    "homepage": "https://github.com/vlucas/spot2",
    "license" : "BSD-3-Clause",
    "authors" : [
        {
            "name": "Vance Lucas",
            "email": "vance@vancelucas.com",
            "homepage": "http://www.vancelucas.com"
        }
    ],
    "require": {
        "php": ">= 8.1.11",
